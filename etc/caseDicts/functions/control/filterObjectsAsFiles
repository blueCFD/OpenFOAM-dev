Description
    Renames objects based on a pattern, specifically designed to work on
  MS Windows.

\*---------------------------------------------------------------------------*/

type            filterObjectsAsFiles;

libs            ("libblueCFDCoreFOs.so");

pattern
(
    // Needed for PDRFoam
    ("B"    "upper_B")
    
    // Needed for potentialFoam
    ("Phi"  "upper_Phi")

    // Needed for all objects that have collons
    (":"    "_collon_")
);

